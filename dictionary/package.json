    "rr-routes": "react-router routes",
    "local-seed": "npx wrangler d1 execute dictionary-db --local --file=drizzle/seed.sql",
    "remote-seed": "npx wrangler d1 execute dictionary-db --remote --file=drizzle/seed.sql"
  },
  "dependencies": {
