{
  "name": "runyoro-rutooro-dictionary",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "cf-typegen": "wrangler types && react-router typegen",
    "check": "tsc && vite build && wrangler deploy --dry-run",
    "deploy": "wrangler deploy",
    "dev": "vite dev",
    "start": "vite preview",
    "typecheck": "npm run typegen && tsc -b",
